# random.wsp -- Random Program
# 
# Initially sets the flow to 30, then squirts random spouts, one at a time.
# User controls duration and period.

name "Random"
set-flow 0 wait 
set-flow 25 wait 
set *duration 50 
set *period 1000

label top 
    random *i 0 12
    squirt *i 
    pause
    goto top

